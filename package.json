{
   "name": "bump-package",
   "description": "Bump package version based on keywords in head commit",
   "version": "2.5.2",
   "main": "lib/main.js",
   "repository": "https://github.com/jpb06/bump-package.git",
   "author": "jpb06 <jpb.06@outlook.com>",
   "license": "MIT",
   "scripts": {
      "cleanDist": "rimraf ./dist && rimraf ./lib",
      "build": "yarn cleanDist && tsc --project tsconfig.prod.json && ncc build",
      "type-check": "tsc --noEmit",
      "lint": "eslint ./src/**/*.ts",
      "test": "jest",
      "test:dev": "jest --watchAll --coverage",
      "test:ci": "jest --ci --coverage",
      "sync-icons": "yarn generateReadmeIcons -h 70"
   },
   "dependencies": {
      "@actions/core": "^1.9.0",
      "@actions/exec": "^1.1.1",
      "@actions/github": "^5.0.3"
   },
   "devDependencies": {
      "@swc/core": "^1.2.213",
      "@swc/jest": "^0.2.21",
      "@types/jest": "^28.1.5",
      "@types/node": "^18.0.3",
      "@typescript-eslint/eslint-plugin": "^5.30.6",
      "@typescript-eslint/parser": "^5.30.6",
      "@vercel/ncc": "^0.34.0",
      "eslint": "^8.19.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-jest": "^26.5.3",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "^28.1.3",
      "jest-watch-typeahead": "^2.0.0",
      "prettier": "^2.7.1",
      "readme-package-icons": "^1.0.5",
      "rimraf": "^3.0.2",
      "typescript": "^4.7.4"
   }
}
